<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkWindow" id="window">
    <property name="title">gjspack demo</property>
    <signal name="close-request" handler="onCloseRequest"/>
    <child>
      <object class="GtkVideo" id="video">
        <property name="autoplay">true</property>
        <property name="loop">true</property>
      </object>
    </child>
    <child>
      <object class="GtkPicture" id="picture"></object>
    </child>
  </object>
</interface>
